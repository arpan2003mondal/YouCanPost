<%- include('./partials/header')%>

<div class="w-full min-h-screen bg-zinc-900 text-white p-10">
    <form autocomplete="off"  action="/users/register" method="post">
        <div class="nav mb-5">
            <a class="text-blue-500 mr-5" href="/users/login">login</a>
            <a class="text-blue-500 mb-15" href="/">Go To Home</a>
        </div>
        <h3 class="text-3xl mb-5">Create Account</h3>
        <input class="px-3 py-2 m-2 bg-transparent rounded-md outline-none border-2 border-zinc-800" type="text" placeholder="name" name="name">
        <input class="px-3 py-2 m-2 bg-transparent rounded-md outline-none border-2 border-zinc-800" type="text" placeholder="username" name="username">
        <input class="px-3 py-2 m-2 bg-transparent rounded-md outline-none border-2 border-zinc-800" type="email" placeholder="email" name="email">
        <input class="px-3 py-2 m-2 bg-transparent rounded-md outline-none border-2 border-zinc-800" type="password" placeholder="password" name="password">
        <input class="px-3 py-2 m-2 bg-transparent rounded-md outline-none border-2 border-zinc-800" type="number" placeholder="age" name="age">
        <input class="px-4 py-2 m-2 bg-blue-500 rounded-md outline-none" type="submit" value="Create User">
    </form>

</div>


<%- include('./partials/footer') %>